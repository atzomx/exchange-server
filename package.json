{
  "name": "graphql-example",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "atzomx",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon ./index.ts",
    "build": "tsc -P ./tsconfig.json",
    "start": "node ./dist"
  },
  "devDependencies": {
    "module-alias": "2.2.2",
    "typescript": "4.7.2"
  },
  "dependencies": {
    "@typegoose/typegoose": "9.8.1",
    "apollo-server-core": "3.8.1",
    "apollo-server-express": "3.8.1",
    "class-validator": "^0.13.2",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "graphql": "15.3.0",
    "mongoose": "6.3.5",
    "nodemon": "2.0.16",
    "type-graphql": "^1.1.1"
  },
  "_moduleAliases": {
    "@core/*": "src/core/*",
    "@core": "src/core",
    "@entities/*": "src/entities/*",
    "@entities": "src/entities",
    "@server/*": "src/server/*",
    "@server": "src/server",
    "@users/*": "src/entities/users/*",
    "@users": "src/entities/users"
